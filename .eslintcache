\r\n      return total + num.count;\r\n    }, 0);\r\n\r\n    return totalCounts;\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Navbar />\r\n\r\n      <Container>\r\n        <SearchInput\r\n          onSubmit={handleSubmit}\r\n          onChange={(e) => setValue(e.target.value)}\r\n          value={value}\r\n        />\r\n\r\n        <Info\r\n          todosLength={todos.length}\r\n          totalCounts={getTotalCoutnts()}\r\n          onDelete={() => setTodos([])}\r\n        />\r\n\r\n        {todos.length > 0 ? (\r\n          <Todos\r\n            todos={todos}\r\n            onSubstraction={(index) => handleAdditionCountMinus(index)}\r\n            onAddition={(index) => handleAdditionCount(index)}\r\n          />\r\n        ) : (\r\n          <div>Kosong</div>\r\n        )}\r\n      </Container>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","D:\\practice\\js\\react-introduction\\starter\\counter-starter\\src\\components\\navbar\\index.js",[],"D:\\practice\\js\\react-introduction\\starter\\counter-starter\\src\\components\\Container\\index.js",[],"D:\\practice\\js\\react-introduction\\starter\\counter-starter\\src\\components\\SearchInput\\indx.js",[],"D:\\practice\\js\\react-introduction\\starter\\counter-starter\\src\\components\\Info\\index.js",[],"D:\\practice\\js\\react-introduction\\starter\\counter-starter\\src\\components\\Todos\\index.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":3,"column":8,"nodeType":"41","messageId":"42","endLine":3,"endColumn":18},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"no-unused-vars","'classnames' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]